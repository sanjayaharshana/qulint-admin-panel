{
    "name": "sanjayaharshana/qulint-admin-panel",
    "description": "Qulint Admin Panel. Open-source Laravel Admin panel. No pro or paid package, free & open. Based on laravel-admin, with special thanks to z-song",
    "type": "library",
    "keywords": ["laravel", "admin", "grid", "form", "qulint-admin","qulint","free"],
    "homepage": "https://github.com/sanjayaharshana/qulint-admin-panel",
    "license": "MIT",
    "version": "3.0.16",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Qulint Team",
            "email": "info@qulint.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/dom-crawler": "^5.0|^6.0|^7.0",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "doctrine/cache": "^2.2",
        "doctrine/annotations": "^2.0",
        "nesbot/carbon": "^2.72.6|^3.8.4"
    },
    "require-dev": {
        "laravel/laravel": "^10.0|^11.0|^12.0",
        "fakerphp/faker": "^1.20",
        "intervention/image": "^2.7|^3.0",
        "laravel/browser-kit-testing": "^7.0|^8.0",
        "spatie/phpunit-watcher": "^1.22.0"
    },
    "autoload": {
        "psr-4": {
            "Qulint\\Admin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Models\\": "tests/models",
            "Tests\\Controllers\\": "tests/controllers"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "sass": "sass --watch resources/assets/qulint-admin/scss/styles.scss:resources/assets/qulint-admin/css/styles.css resources/assets/qulint-admin/scss/pages:resources/assets/qulint-admin/css/pages --style compressed",
        "test": "./vendor/bin/phpunit"
    },
    "suggest": {
        "intervention/image": "Required to handling and manipulation upload images (^2.7|^3.0).",
        "spatie/eloquent-sortable": "Required to built orderable gird.",
        "doctrine/dbal": "Optional: For advanced database operations and schema management.",
        "carbonphp/carbon-doctrine-types": "Optional: For Carbon integration with Doctrine (compatible versions)"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qulint\\Admin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Qulint\\Admin\\Facades\\Admin"
            }
        }
    },
    "conflict": {
        "doctrine/dbal": "<3.0",
        "nesbot/carbon": "<2.72.6"
    },
    "config": {
        "process-timeout": 0
    }
}
